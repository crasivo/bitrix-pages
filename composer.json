{
  "name": "crasivo/bitrix-pages",
  "type": "bitrix-d7-module",
  "license": "MIT",
  "authors": [
    {
      "email": "crasivodev@gmail.com",
      "name": "Crasivo Dev",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Crasivo\\Pages\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "composer/installers": ">=2.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}